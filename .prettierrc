{
  "plugins": ["prettier-plugin-tailwindcss"],

  "singleQuote": false,
  "bracketSameLine": false,
  "bracketSpacing": true,
  "jsxSingleQuote": false,
  "useTabs": false,
  "trailingComma": "all",
  "semi": true,
  "singleAttributePerLine": true,
  "arrowParens": "always",
  "quoteProps": "as-needed",
  "tabWidth": 2
}
